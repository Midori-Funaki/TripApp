{{#booking_details}}
<li class="list-group-item swappable-block">
    <h5 class="swappble-block-title-hotel">
        Hotel
    </h5>
    <i class="fa fa-times fa-1x delete-activity"></i>

    <div class="swappble-block-content-transit">
            <a class="hotel-title"><strong>{{hotelName}}</strong></a>
            <div class="hotel-info">
                <span>{{check_in}} - {{check_out}}</span>
            </div>
    </div>
    <input name="hotel-data" class="hidden-input-data" type="hidden" value={{EncodeJSON2string ../this}}>
</li>
{{/booking_details}}
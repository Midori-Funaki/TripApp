{{#map_result}}
<li class="list-group-item swappable-block">
    <h5 class="swappble-block-title-location">
        Location
    </h5>
    <i class="fa fa-times fa-1x delete-activity"></i>
    <div class="swappble-block-content-transit">
            <a class="location-title"><strong>{{name}}</strong></a><br>
            <p>{{address}}</p>
    </div>
    <input name="location-data" class="hidden-input-data" type="hidden" value={{EncodeJSON2string ../this}}>
</li>
{{/map_result}}
